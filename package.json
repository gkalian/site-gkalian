{
  "name": "gk-site",
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@mui/types": "^7.4.5",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/github": "^10.3.4",
    "semantic-release": "^24.2.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Changelog\n\nAll notable changes to this project will be documented in this file."
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "\n          # Install yq if missed\n          if ! command -v yq &> /dev/null; then\n            wget -qO /tmp/yq https://github.com/mikefarah/yq/releases/latest/download/yq_linux_amd64\n            chmod +x /tmp/yq\n            YQ_CMD=\"/tmp/yq\"\n          else\n            YQ_CMD=\"yq\"\n          fi\n          \n          #  Update Chart.yaml with yq\n          $YQ_CMD eval '.version = \"${nextRelease.version}\"' -i helm/Chart.yaml\n          $YQ_CMD eval '.appVersion = \"${nextRelease.version}\"' -i helm/Chart.yaml\n          "
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md",
            "helm/Chart.yaml"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\\n\\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
